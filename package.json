  "bugs": {
    "url": "https://github.com/Richard-Burd/alpine-js-sandbox/issues"
  },
  "homepage": "https://github.com/Richard-Burd/alpine-js-sandbox#readme",
  "dependencies": {
    "alpinejs": "^3.3.3",
    "express": "^4.17.1",
    "tailwindcss": "^2.2.16"
  }
}
