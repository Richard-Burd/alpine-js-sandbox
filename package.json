{
  "name": "alpine-js-sandbox",
  "version": "1.0.0",
  "description": "Full-stack Test App using Alpine.js",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "build": "npm run build-css:prod",
